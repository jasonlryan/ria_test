{
  "version": 2,
  "name": "workforce-survey-2025",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/embed/asst_D0BPAJjvg3UK6Lcb1lqIM1xS",
      "permanent": false
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "regions": [
    "iad1"
  ],
  "functions": {
    "app/api/chat-assistant/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "app/api/query/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "app/api/retrieve-data/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "app/api/test-openai/route.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "app/api/test-assistant/route.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "app/api/test-key/route.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
